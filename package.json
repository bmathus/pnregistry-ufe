{
  "name": "pnregistry-ufe",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/pnregistry-ufe/pnregistry-ufe.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "convert-openapi": "js-yaml  ./api/pnregistry.openapi.yaml > .openapi.json",
    "mock-api": "open-api-mocker --schema .openapi.json --port 5005",
    "start:app": "stencil build --dev --watch --serve",
    "start:mock": "run-s convert-openapi mock-api",
    "start": "run-p -r start:mock start:app",
    "build": "stencil build",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "openapi": "openapi-generator-cli generate"
  },
  "dependencies": {
    "@material/web": "^1.3.0",
    "@stencil/core": "^4.7.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "open-api-mocker": "^2.0.0",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT"
}
